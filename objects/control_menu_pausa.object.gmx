    <event eventtype="2" enumb="3">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Manejo de la cancelación de opciones.

// Varían según el room 
switch( room ){

    // &lt;&lt;&lt; MENÚ PRINCIPAL &gt;&gt;
    case room_menu_principal:{

        // Varían según que sub-menú se encuentra.
        switch( menu_submenu ){
        
            // Menú Principal
            case 0:{
            
                // Se detiene cualquier posibilidad que se habilite el poder cambiar de opciones en el menú de configuración.
                alarm[1] = -1;
                menu_move = false;
            
                alarm[1] = 10; //Se habilita el poder cambiar de opciones.
                 
                // Se va a la priera opción
                menu_opcion = 0;
                
                break;
            
            }
            
            // Controles
            case 1:{
            
                
                
                    // Se detiene cualquier posibilidad que se habilite el poder cambiar de opciones en el menú de configuración.
                    alarm[1] = -1;
                    menu_move = false;
                                
                                
                    // Se eliminan los carteles que señalan la configuraciòn de control y se muestra la confirmación del cambio.
                    with(ob_carteles){ 
                    
                        if( tipo == 2 or tipo == 3 or tipo == 4 ){ instance_destroy(); }
                        
                    }
                    
                    alarm[1] = 10; //Se habilita el poder cambiar de opciones.
                
                    // Se setean las opciones donde se estaba antes de entrar al submenú de controles.
                    menu_submenu = 0;
                    menu_opcion = 1
                    cantidad_opciones= 5;
                    
                    // Se posiciona el puntero.
                    x_puntero = 32 + sprite_get_width(sp_cartel_menu) + 20;
                    y_puntero = ( ( sprite_get_height(sp_cartel_menu) - 32 ) / 2 ) + 16;
                
                break;
            
            }
            
            // Creditos
            case 2:{
            
                alarm[1] = -1;
                efectos( 0, 2, c_black ); // Efecto negro para transicion.                        
                alarm[6] = 6; // Cuando se activa la alarma se produce el cambio y lo que se necesita para volver al menu principal.
                
                //[Sonido]
                audio_play_sound( sn_menu_volver, 10, false );
            
                break;
            
            }
        
        }
        
        
        //[Sonido]
        audio_play_sound( sn_menu_cancel, 10, false );
        
        break;
    
    }
    
    // &lt;&lt;&lt; SELECCIÓN SECTORES &gt;&gt;
    case room_seleccion_sectores:{
    
        // Se quita la planilla del bloque sector que se estaba
        with( ob_bloque_sector ){
        
            if( other.menu_opcion == numero ){ mostrar_planilla = false; }
        
        }
    
        // Se va a la opción de volver atras posicionando el puntero.
        menu_opcion = 0;
        
        x_puntero = 16 + sprite_get_width(sp_cartel_colgante_chico) + 20;
        y_puntero = 8 + ( sprite_get_height(sp_cartel_colgante_chico) / 2 );
        
        //[Sonido]
        audio_play_sound( sn_menu_cancel, 10, false );
    
        break;
    
    }
    
    // &lt;&lt;&lt; SELECCIÓN NIVELES &gt;&gt;
    case room_seleccion_niveles:{
    
        // Se quita la planilla del bloque sector que se estaba
        with( ob_bloque_nivel ){
        
            if( other.menu_opcion == numero ){ mostrar_planilla = false; }
        
        }
    
        // Se va a la opción de volver atras posicionando el puntero.
        menu_opcion = 0;
        
        x_puntero = 16 + sprite_get_width(sp_cartel_colgante_chico) + 20;
        y_puntero = 8 + ( sprite_get_height(sp_cartel_colgante_chico) / 2 );
        
        //[Sonido]
        audio_play_sound( sn_menu_cancel, 10, false );
    
        break;
    
    }
    
    // &lt;&lt;&lt; NIVELES / OTROS &gt;&gt;
    default:{
    
        // Se saca el cartel de pausa
        with(ob_carteles){
        
            if( tipo == 9 ){ instance_destroy(); break; }
        
        }
        
        // Se vuelve al estado normal del menu.
        menu_opcion = 0;
        menu_on = false;
        en_pausa = false;
        
        //--&gt; Se activan las instancias.
        instance_activate_all();
        
        // La orbes que ya habian sido agarradas, se vuelven a desactivar.
        with( ob_orbe ){
        
            if( agarrado == true ){ instance_deactivate_object(self); }
        
        }
        
        // Se retorna el volumen que se tenía.
        audio_sound_gain( control_general.musica_principal, volumen_musica, 0 );
         
        // Antes de destruir el bojeto "surface_pausa", se borra el sprite que creo.
        with( surface_pausa ){
        
            sprite_delete( sprite_fondo );
            instance_destroy();
        
        }
        
        // Si se esta en el sector extra y hay un efecto de rayo desactivado (flasheo) con un alpha distinto de cero, se vuelve a activar.
        if( sector_actual == 7 ){
        
            with( control_efectos ){
            
                if( alpha != 0 and activado == false ){ activado = true; }
            
            }
            
        }
        
        // Se llama a la alarma que habilita hacer pausa de nuevo
        alarm[4] = 5;
        
        //[Sonido]
        audio_play_sound( sn_pausa_off, 10, false );
    
    }

}
</string>
