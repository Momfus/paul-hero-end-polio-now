            <string>/// Poner pausa

// Se verifica que el jugador presione la tecla correspondiente, que este habilitado el poder hacer pausa y que no se esté en la etapa de terminar nivel.
if( can_pausa == true and keyboard_check_released(tecla_salir)  ){

    // Se detiene los movimientos de fondo que tenga el background
    background_detener(true);
    
    // Se borra cualquier efecto de particular en pantalla.
    effect_clear();

    // Se detienen todos los que sean soniidos.
    audio_group_stop_all( audiogroup_default );
    
    // Se pone la mitad del volumen que está la música.
    audio_sound_gain( control_general.musica_principal, ( volumen_musica / 2 ), 0 );
    
    /* Se crea el objeto que realiza el efecto de desenfoque en pantalla al hacer pausa (crea un sprite pero hace uso de los surface) y detiene
    los demas objetos. */
    instance_create( 0, 0, surface_pausa );
    
    // Se cambian los atributos necesarios para señalar que está en pausa.
    can_pausa = false;
    en_pausa = true;

    // Se colocan los valores iniciales del puntero.
    x_puntero = room_centro_x - ( sprite_get_width( sp_cartel_pausa ) / 2 ) + 32;
    y_puntero = room_centro_y + ( sprite_get_height( sp_cartel_pausa ) / 2 ) + 20;
    
    // Se crea el cartel correspondiente en medio de la pantalla.
    var pos_x = room_centro_x - ( sprite_get_width(sp_cartel_pausa) / 2),
        pos_y = room_centro_y + ( sprite_get_height(sp_cartel_pausa) / 2);
    
    drop_cartel(9, pos_x, pos_y - 32, pos_y);
    
    alarm[0] = 5; // Se habilita el moverse en el menú de la pausa.
    
    
    //[Sonido]
    audio_play_sound( sn_pausa_on, 10, false );
    

}
