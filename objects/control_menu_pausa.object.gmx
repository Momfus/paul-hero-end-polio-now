            <string>/// Puntero

// El puntero se dibuja cuando los carteles terminan la animación de movimiento al rebotar.
if( menu_on == true ){ 
    
    image_speed = 0.1;
    
    // Se considera el room en que se encuentra.
    switch(room){
    
        // &lt;&lt;&lt; MENÚ PRINCIPAL &gt;&gt;&gt;
        case room_menu_principal:{
        
            // Según el submenu en que se encuenta.
            switch( menu_submenu ){
            
                // Menú principal
                case 0:{
                
                    var posicion_y_puntero = y_puntero * ( menu_opcion + 1 ) + 48 * menu_opcion  - 1;
                    
                    switch(menu_opcion){
                    
                        // Jugar
                        case 0: { 
                        
                            draw_sprite( sp_puntero, 0, x_puntero , posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                        
                        //Controles
                        case 1: { 
                        
                            draw_sprite( sp_puntero, 0, x_puntero , posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32,  posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                           
                        //Sobre Créditos
                        case 2: { 
                        
                            draw_sprite( sp_puntero, 0, x_puntero , posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                        
                        //Sobre PolioPlus
                        case 3: { 
                            
                            draw_sprite( sp_puntero, 0, x_puntero , posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                        
                        
                        //Ir página Rotary
                        case 4: { 
                        
                            draw_sprite( sp_puntero, 0, x_puntero, posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                        
                        //Ir página Crios
                        case 5: { 
                        
                            draw_sprite( sp_puntero, 0, x_puntero, posicion_y_puntero ); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, posicion_y_puntero ); 
                            
                            break; 
                            
                        }
                    }
                
                    break;
                    
                }
                
                //Controles
                case 1:{
                
                    // El puntero no se muestra cuando el mensaje de confirmación se esta mostrando.
                    if( menu_opcion != 3 ){
                            
                        // 0 = WASD + J / K - 1 = Flechas + X / Z
                        if( menu_opcion == 0 ){
                                
                            draw_sprite( sp_puntero, 0, x_puntero , y_puntero); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, y_puntero);
                                        
                        }
                        else{
                                
                            var cartel_opcion_flechas_centro = y_puntero + sprite_get_height(sp_cartel_normal) + 50;    
                                    
                            draw_sprite( sp_puntero, 0, x_puntero , cartel_opcion_flechas_centro); 
                            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, cartel_opcion_flechas_centro);
                                    
                        }
                        
                    
                    }
                    
                    break;
                
                }
                
                // Créditos
                case 2:{
                
                    draw_sprite( sp_puntero, 0, x_puntero , y_puntero ); 
                    draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, y_puntero ); 
                
                    break;
                
                }
                    
            }
            
            break;
        
        }
        
        // &lt;&lt;&lt; SELECCIÓN SECTORES &gt;&gt;&gt;
        case room_seleccion_sectores:{
        
            draw_sprite( sp_puntero, 0, x_puntero, y_puntero ); 
            draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, y_puntero ); 
                            
                            
            break;
        
        }
        
         // &lt;&lt;&lt; SELECCIÓN NIVELES&gt;&gt;&gt;
        case room_seleccion_niveles:{
        
            if( menu_opcion != 13 ){
            
                draw_sprite( sp_puntero, 0, x_puntero, y_puntero ); 
                draw_sprite( sprite_tecla_aceptar, -1, x_puntero + 32, y_puntero ); 
                
            }
            else{
            
                draw_sprite_ext( sp_puntero, 0, x_puntero, y_puntero, -1, 1, 0, c_white, 1 ); 
                draw_sprite( sprite_tecla_aceptar, -1, x_puntero - 32, y_puntero );
            
            }
                            
                            
            break;
        
        }
        
        // &lt;&lt;&lt; INTRO con configuración de controles &gt;&gt;&gt;
        case room_intro:{
        
            // El puntero no se muestra cuando el mensaje de confirmación se esta mostrando.
            if( menu_opcion != 3 ){
            
                draw_set_color(c_white);
                draw_set_halign(fa_left);
                draw_set_valign(fa_middle);
                                
                draw_sprite( sp_puntero, 0, x_puntero , y_puntero); 
                draw_sprite( sp_key_J_menu, -1, x_puntero + 32, y_puntero);
                draw_text( x_puntero + 56, y_puntero, "/");
                draw_sprite( sp_key_X_menu, -1, x_puntero + 88, y_puntero );
        
            }
             
            break;
        
        }
        
        // &lt;&lt;&lt; En los niveles (cuando se hace PAUSA) &gt;&gt;&gt;
        default:{
            
            draw_sprite_ext( sp_puntero_pausa, 0, x_puntero, y_puntero, 1, 1, -90, -1, 1 );
            draw_sprite( sprite_tecla_jump, -1, x_puntero, y_puntero + 32 );
        
        }
        
    
    
    }

}
        



