    <event eventtype="2" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Manejo de la selección de opciones.

// Se varía según el room que se encuentra.
switch(room){

    // &lt;&lt;&lt; MENÚ PRINCIPAL &gt;&gt;
    case room_menu_principal:{
    
        // Varían según que sub-menú se encuentra.
        switch( menu_submenu ){
        
            // Menú Principal
            case 0:{
            
                // Dependiendo en la opción que se esté, es lo que se realiza.
                switch(menu_opcion){
                
                    // Comenzar a Jugar
                    case 0:{
                    
                        menu_on = false;
                        menu_move = false;
                        cantidad_opciones = 6;
                        
                        transicion( room_next(room), 0, 2);
                    
                        
                        break;
                    
                    }
                    
                    // Controles
                    case 1:{
                    
                        // Crea los carteles correspondientes a la configuración del control.
                        menu_configuracion_controles( room_centro_x + 80, 180);
                        
                        menu_submenu = 1;
                        menu_opcion = 0;
                        cantidad_opciones = 1;
                        
                        break;
                    
                    }
                    
                    // Mostrar los créditos
                    case 2:{
                    
                        // Se simula que se va a otra habitación pero lo que se hace es borrar lo que hay en pantalla y mostrar los créditos.
                    
                        efectos( 0, 2, c_black ); // Efecto negro para transicion.                        
                        alarm[6] = 6; // Cuando se activa la alarma se produce el cambio y lo que se necesita para los creditos.
                        
                        break;
                    
                    }
                    
                    // Ir página PolioPlus
                    case 3:{ 
                    
                        url_open_ext( 'http://www.endpolio.org', '_blank'); 
                        alarm[1] = 5; // Habilita el moverse por el menú nuevamente
                        
                        break; 
                        
                    }
                    
                    
                    // Ir página Rotary International
                    case 4:{ 
                    
                        url_open_ext( 'https://www.rotary.org', '_blank'); 
                        alarm[1] = 5; // Habilita el moverse por el menú nuevamente
                        
                        break; 
                        
                    }
                    
                    // Ir página Crios Devs
                    case 5:{ 
                    
                        url_open_ext( 'http://www.criosdevs.com/', '_blank'); 
                        alarm[1] = 5; // Habilita el moverse por el menú nuevamente
                        
                        break; 
                        
                    }
                    
                    default:{
                    
                        show_message("Opción aún no disponible");
                        menu_move = true;
                    
                        break;
                        
                    }
                
                }
                
                break;
            
            }
            
            // Controles
            case 1:{
            
                // 0 = WASD + J / K - 1 = Flechas + X / Z - 3 = Aviso de guardado de nueva configuración.
                if( menu_opcion != 3 ){
                
                    // Se detiene cualquier posibilidad que se habilite el poder cambiar de opciones en el menú de configuración.
                    alarm[1] = -1;
                    menu_move = false;
                                
                    // Se realiza la configuración necesaria y se señala que se realizió la configuración correctamente
                    setear_controles(menu_opcion);
                    cargar_controles();
                    menu_opcion = 3; // No se dibuja el puntero
                                
                    // Se eliminan los carteles que señalan la configuraciòn de control y se muestra la confirmación del cambio.
                    with(ob_carteles){ 
                    
                        if( tipo == 2 or tipo == 3 ){ instance_destroy(); }
                        
                    }
                    
                    var medio_y_cartel_confirmacion = 180 + ( sprite_get_height(sp_cartel_normal) * 1.5 );
                    drop_cartel(4, room_centro_x + 80, medio_y_cartel_confirmacion - 20, medio_y_cartel_confirmacion ); 
            
                }
                else{
                
                    // Se elimina el cartel de aviso de guardado de configuración.
                    with(ob_carteles){
                    
                        if( tipo == 4 ){ instance_destroy(); }
                    
                    }
                
                    alarm[1] = 10; //Se habilita el poder cambiar de opciones.
                
                    // Se setean las opciones donde se estaba antes de entrar al submenú de controles.
                    menu_submenu = 0;
                    menu_opcion = 1
                    cantidad_opciones= 5;
                    
                    // Se posiciona el puntero.
                    x_puntero = 32 + sprite_get_width(sp_cartel_menu) + 20;
                    y_puntero = ( ( sprite_get_height(sp_cartel_menu) - 32 ) / 2 ) + 16;
                    
                }
                
                break;
            
            }
            
            // Créditos
            case 2:{
            
                alarm[1] = -1;
                efectos( 0, 2, c_black ); // Efecto negro para transicion.                        
                alarm[6] = 6; // Cuando se activa la alarma se produce el cambio y lo que se necesita para volver al menu principal.
                
                //[Sonido]
                audio_play_sound( sn_menu_volver, 10, false );
            
                break;
            
            }
        
        }
        
        //[Sonido]
        audio_play_sound( sn_menu_valido, 10, false );
        
        break;
    
    }
    
    
    // &lt;&lt;&lt; SELECCIÓN SECTORES &gt;&gt;
    case room_seleccion_sectores:{
    
        /* Si no está la opción de volver (cuando la opción no es cero), se verifica si el sector no esta bloqueado y en el caso que no lo sea se va a 
        la selección de niveles con los datos correspondientes. Si está sobre la opción volver se retorno al menú principal.*/
        if( menu_opcion != 0 ){
        
            // Se verifica si el sector seleccionado no esta bloqueado.
            with(ob_bloque_sector){
             
                if( numero == other.menu_opcion  ){
                
                    if( desbloqueado == true ){
                    
                        with(other){
                        
                            alarm[1] = -1; // Se cancela el llamado de esta alarma y el atributo menu_move se activa al iniciar la siguiente pantalla.
                        
                            aux_sector_actual = menu_opcion;
                            nivel_actual = 1;
                            cantidad_opciones = 13;
                            menu_move = false;
                            menu_on = false;
                            
                            transicion(room_seleccion_niveles, 0, 2);
                            
                        }
                        
                        //[Sonido]
                        audio_play_sound( sn_menu_valido, 10, false );
                    
                    }
                    else{
                
                        // Si no esta vibrando, se realiza esa animación y se reproduce el sonido correspondiente.
                        if( timeline_running == false ){
        
                            timeline_position = 0;                
                            timeline_running = true;
         
                            audio_play_sound( sn_menu_invalido, 10, false ); //[Sonido]
                            
                        }
                        
                    }
                    
                    break;
                
                }
                
            
            }
        
        }
        else{ // Se retorna al menú principal
        
            alarm[1] = -1;
        
            menu_on = false;
            aux_sector_actual = 1;
            cantidad_opciones= 5;
            
            transicion( room_menu_principal, 0, 2);
            
            //[Sonido]
            audio_play_sound( sn_menu_volver, 10, false );
        
        }
        
        break;
    
    }
    
    // &lt;&lt;&lt; SELECCIÓN NIVELES &gt;&gt;
    case room_seleccion_niveles:{
    
        /* Si no está la opción de volver (cuando la opción no es cero), se verifica si el nivel no está bloqueado y en el caso que no lo sea se va al
        nivel correspondiente. Si está sobre la opción volver se retorna a la seleccion de sectores.*/
        if( menu_opcion != 0 ){
        
            switch(menu_opcion){
            
                case 11:{ // Se va al nivel especial (si no está bloqueado).
                
                    // Se verifica si el nivel seleccionado no esta bloqueado.
                    with(ob_bloque_nivel){
                    
                        if( numero == other.menu_opcion  ){
                        
                            if ( desbloqueado == true ){
                            
                                with(other){
                                
                                    nivel_actual = menu_opcion;
                                    
                                    menu_opcion = 0;
                                    cantidad_opciones = 2;
                                    alarm[1] = -1;
                                    menu_move = false;
                                    menu_on = false;
                                    
                                    var nombre_room = "nivel_extra_" + string(sector_actual);
                                    transicion(asset_get_index(nombre_room), 0, 2);
                                    
                                    //Se llama a la alarma que activa la posibilidad de hacer pausa.
                                    alarm[4] = 15;
                                    
                                }
                                
                                //[Sonido]
                                audio_play_sound( sn_menu_valido, 10, false );
                            
                            }
                            else{
                            
                                // Si no esta vibrando, se realiza esa animación y se reproduce el sonido correspondiente.
                                if( timeline_running == false ){
                
                                    timeline_position = 0;                
                                    timeline_running = true;
                 
                                    audio_play_sound( sn_menu_invalido, 10, false ); //[Sonido]
                                    
                                }
                            
                            }
                            
                            break;
                            
                        }
                    
                    }
                
                    break;
                
                }
                
                case 12:{ // Se va al anterior sector.
                
                    alarm[1] = -1;
                    nivel_actual = 1;
                    menu_on = false;
                
                    aux_sector_actual = sector_actual - 1;
                    transicion( room_seleccion_niveles, 0, 2);
                    
                    //[Sonido]
                    audio_play_sound( sn_menu_valido, 10, false );
                
                    break;
                
                }
                
                case 13:{ // Se va al siguiente sector.  
                
                    alarm[1] = -1;
                    nivel_actual = 1;
                    menu_on = false;
                
                    aux_sector_actual = sector_actual + 1;
                    transicion( room_seleccion_niveles, 0, 2);
                
                    //[Sonido]
                    audio_play_sound( sn_menu_valido, 10, false );
                    
                    break;
                
                }
                
                default:{ // Selección de los niveles normales (si no están bloqueados).
             
                  // Se verifica si el nivel seleccionado no esta bloqueado.
                    with(ob_bloque_nivel){
     
                        if( numero == other.menu_opcion ){
               
                            if( desbloqueado == true ){
                            
                                with(other){
                                
                                    nivel_actual = menu_opcion;
                                    
                                    menu_opcion = 0;
                                    cantidad_opciones = 2;
                                    alarm[1] = -1;
                                    menu_move = false;
                                    menu_on = false;
                                    
                                    //Se llama a la alarma que activa la posibilidad de hacer pausa.
                                    alarm[4] = 15;
                                    
                                    // Se verifica si es el último nivel (el nombre es distinto porque varian un par de cosas).
                                    if( nivel_actual != 10 ){ var nombre_room = "nivel_" + string(nivel_actual + 10 * ( sector_actual - 1 ) ); }
                                    else{ var nombre_room = "nivel_final_" + string(sector_actual); }
                                    
                                    transicion(asset_get_index(nombre_room), 0, 2);
                                    
                                }
                                
                                //[Sonido]
                                audio_play_sound( sn_menu_valido, 10, false );
                                
                            }
                            else{
                        
                                // Si no esta vibrando, se realiza esa animación y se reproduce el sonido correspondiente.
                                if( timeline_running == false ){
                
                                    timeline_position = 0;                
                                    timeline_running = true;
                 
                                    audio_play_sound( sn_menu_invalido, 10, false ); //[Sonido]
                                    
                                }
                        
                            }
                            
                            break;
                                                    
                        }
                        
                    
                    }  
                    
                    break;    
                
                }
            
            }
        
        }
        else{ // Se retorna a la selección de sectores
        
            alarm[1] = -1;
        
            menu_on = false;
            cantidad_opciones = 6;
            
            transicion( room_seleccion_sectores, 0, 2);
            
            //[Sonido]
            audio_play_sound( sn_menu_volver, 10, false );
            
        }
        
        break;
    
    }
    
    // &lt;&lt;&lt; NIVELES / OTROS &gt;&gt;
    default:{
    
        switch( menu_opcion ){
    
            // Se reanuda el juego 
            case 0:{
    
                // Se llama a la alarma que maneja la cancelación ya que hace lo mismo.
                event_perform( ev_alarm, 3 );
                
                //[Sonido]
                audio_play_sound( sn_pausa_off, 10, false );
                    
                break;
            
            }
            
            // Reiniciar nivel
            case 1:{
            
                menu_on = false; // Se desactiva por un tiempo el poder usar un menu para que se desarrolle el efecto anterior..
                
                // Se usa una linea de tiempo debido a que se deben realizar en un orden
                timeline_index = time_pausa_reinicio;
                timeline_position = 0;
                timeline_running = true; 
                
                //[Sonido]
                audio_play_sound( sn_menu_valido, 10, false );
                
                break;    
            
            }
            
            // Volver a la selección de niveles
            case 2:{
            
                transicion( room_seleccion_niveles, 0, 2 );  
                menu_on = false; // Se desactiva por un tiempo el poder usar un menu para que se desarrolle el efecto anterior.
                
                alarm[5] = 5; // Antes de completar la transición se borra el sprite y se señala que ya no se está en pausa.
                
                //[Sonido]
                audio_play_sound( sn_menu_volver, 10, false );
                
                break;
            
            }
        
        }
        
        break;
    
    }

}
