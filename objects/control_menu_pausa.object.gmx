                    alarm[1] = 10;
                    
                    //[Sonido]
                    audio_play_sound( sn_menu_move, 10, false );
                        
                }
                
                // Dependiendo en que posición del menú está, es lo que se setea para configurar los controles en el archivo ".ini" correspondiente.
                if( tecla_menu_seleccion ){
        
                    //[Sonido]
                    audio_play_sound( sn_menu_valido, 10, false );
                
                    // Se detiene cualquier posibilidad que se habilite el poder cambiar de opciones en el menú de configuración.
                    alarm[1] = 30; 
                    menu_move = false;
                    
                    // Se realiza la configuración necesaria y se señala que se realizió la configuración correctamente
                    setear_controles(menu_opcion);
                    cargar_controles();
                    menu_opcion = 3;
                    
                    // Se eliminan los otros carteles y se muestra la confirmación del cambio.
                    with(ob_carteles){ instance_destroy(); }
                    var medio_room_y_cartel = room_centro_y + ( sprite_get_height(sp_cartel_normal) / 2 );
                    drop_cartel(4, room_centro_x - ( sprite_get_width(sp_cartel_normal) / 2 ), medio_room_y_cartel - 20, medio_room_y_cartel ); 
                    
                        
                }
                
            }
        
        }
        else{
                
            // Vefifica si se presiona la tecla de selección elegida en la configuración para confirmar la misma.
            tecla_menu_seleccionar = keyboard_check_released(tecla_jump);
            
            // Se vuelve al principio del room al presionar la tecla de seleccion.
            if( tecla_menu_seleccionar == true ){ 
            
                menu_on = false;
                menu_move = false;
                transicion(room, 0, 2); 
            
                //[Sonido]
                audio_play_sound( sn_menu_valido, 10, false );
                    
            }
                
        }
        
        // Si se estaba moviendo hacia arriba en el menú y se suelta alguna de las teclas, se vuelve falso.
        if( tecla_menu_arriba == true ){  
            
            if( keyboard_check_released(ord('W')) or keyboard_check_released(vk_up) ){ tecla_menu_arriba = false; }
            
        }
            
        // Si se estaba moviendo hacia abajo en el menú y se suelta alguna de las teclas, se vuelve falso.
        if( tecla_menu_abajo == true ){
            
            if( keyboard_check_released(ord('S')) or keyboard_check_released(vk_down) ){ tecla_menu_abajo = false; }
                
        }
        
    
    }

}
