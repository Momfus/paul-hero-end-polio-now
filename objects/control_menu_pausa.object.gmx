    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Manejo del jugador en el menú

// Se activa el manejo del menú cuando la variable correspondiente este en verdadero.
if( menu_on == true ){

    // Se verifica que no se esté en la intro, ya que ahí se toman otras medidas especiales para la configuración.
    if( room != room_intro ){
    
        // Según en que "room" se encuentra el jugador, es el menú a controlar.
        switch( room ){
            
            /*--------------------------
            
                &lt;&lt;&lt; MENÚ PRINCIPAL &gt;&gt;&gt;
            
            ---------------------------*/
            case room_menu_principal: {
                
                // Se guardan las entradas de teclado para aceptar o cancelar una acciones
                tecla_menu_seleccionar = keyboard_check_released(tecla_jump);
                tecla_menu_volver = keyboard_check_released(tecla_salir);
                
                // Se controla que las ambas teclas anteriores no puedan actuar a la vez
                if( tecla_menu_volver == true and tecla_menu_seleccionar == true ){
                
                    tecla_menu_volver = false;
                    tecla_menu_seleccionar = false;
                
                }
                 
                // Se verica si el jugador puede moverse en el menú.
                if( menu_move == true ){
                    
                    // El movimienot no se hace cuando se esta en el submenu de crèditos (cuando la variable es igual a 2)
                    if( menu_submenu != 2 ){
                    
                        // Se guardan las entradas de teclado para movimiento necesarias
                        tecla_menu_arriba = keyboard_check(tecla_up);
                        tecla_menu_abajo = keyboard_check(tecla_down);
                        
                            
                        // En el caso que valgan verdadero ambas teclas (arriba y abajo), se toman como falsas luego.
                        if( tecla_menu_arriba and tecla_menu_abajo ){
                            
                            tecla_menu_arriba = false;
                            tecla_menu_abajo = false;
                            
                        }
                            
                        // Al moverse hacia arriba
                        if( tecla_menu_arriba == true ){
                        
                            if( menu_opcion != 0 ){ menu_opcion -= 1; }
                            else{  menu_opcion = cantidad_opciones;  } 
                                
                            // Se inhabilita temporalmente el movimiento del menu.
                            menu_move = false;
                            alarm[1] = 10;
                        
                            //[Sonido]
                            audio_play_sound( sn_menu_move, 10, false );
                                
                            break;
                                
                        }
                            
                        // Al moverse hacia abajo
                        if( tecla_menu_abajo == true ){
                            
                            if( menu_opcion != cantidad_opciones ){ menu_opcion += 1; }
                            else{  menu_opcion = 0;  }
                                
                            // Se inhabilita temporalmente el movimiento del menu.
                            menu_move = false;
                            alarm[1] = 10;
                            
                            //[Sonido]
                            audio_play_sound( sn_menu_move, 10, false );
                            
                            break;
                            
                        }
                        
                    }
                     
                }
                   
                // Al seleccionar una opción
                if( tecla_menu_seleccionar == true and control_efectos.activado == false){
                        
                    // Se desactiva temporalmente el uso del menu y se llama a la alarma que manejo el uso de opciones
                    menu_move = false;
                    event_perform(ev_alarm, 2);
                    
                    break;
                        
                }
                
                // Al cancelar una opción o volver de un submenu.
                if( tecla_menu_volver == true and ( menu_opcion != 0 or menu_submenu != 0) and control_efectos.activado == false){
                
                    // Se desactiva temporalmente el uso del menu y se llama a la alarma que manejo la cancelación de opciones.
                    menu_move = false;
                    event_perform(ev_alarm, 3);
                    
                    break;
                
                }
                    
                break;
                
            }
            
             /*--------------------------------
            
                &lt;&lt;&lt; SELECCIÓN DE SECTORES &gt;&gt;&gt;
            
            -----------------------------------*/
            case room_seleccion_sectores: {
            
                // Se guardan las entradas de teclado para aceptar o cancelar una acciones
                tecla_menu_seleccionar = keyboard_check_released(tecla_jump);
                tecla_menu_volver = keyboard_check_released(tecla_salir);
            
                // Se controla que las ambas teclas anteriores no puedan actuar a la vez
                if( tecla_menu_volver == true and tecla_menu_seleccionar == true ){
                
                    tecla_menu_volver = false;
                    tecla_menu_seleccionar = false;    
                
                }
            
                // Se verica si el jugador puede moverse en el menú.
                if( menu_move == true ){
                
                    // Se guardan las entradas de teclado para movimiento necesarias
                    tecla_menu_arriba = keyboard_check(tecla_up);
                    tecla_menu_abajo = keyboard_check(tecla_down);
                    tecla_menu_izquierda = keyboard_check(tecla_left);
                    tecla_menu_derecha = keyboard_check(tecla_right);                    
                        
                    // En el caso que valgan verdadero ambas teclas (arriba y abajo), se toman como falsas luego.
                    if( tecla_menu_arriba == true and tecla_menu_abajo == true ){
                        
                        tecla_menu_arriba = false;
                        tecla_menu_abajo = false;
                        
                    }
                    
                    // En el caso que valgan verdadero ambas teclas (izquierda y derecha), se toman como falsas luego.
                    if( tecla_menu_izquierda and tecla_menu_derecha ){
                    
                        tecla_menu_izquierda = false;
                        tecla_menu_derecha = false;
                    
                    }

                    //----&gt; Al moverse hacia arriba
                    if( tecla_menu_arriba == true ){                    
                    
                        //Se verifica si está en la primera fila de sectores (menor a 4), segunda fila (mayor a 4) o la opción volver (igual a 0).
                        if( menu_opcion == 0 ){ // primera opcion de la segunda fila
                        
                            menu_opcion = 6; 
                            
                            // Se consigue la posición del bloque de sector 6 (especial)
                            with( ob_bloque_sector ){
                            
                                if( numero == 6 ){
                                    
                                    // Posicionamiento del puntero.
                                    other.x_puntero = x + sprite_get_width(sp_bloque_sector) + 20; 
                                    other.y_puntero = y + ( sprite_get_height(sp_bloque_sector) / 2 );
                                    
                                }
                                
                            }
                            
                        } 
                        else{
                        
                            if( ( menu_opcion - 3 )  &lt;= 0 ){ // a la opción de volver atrás.
                            
                                menu_opcion = 0;
                                 
                                // Posicionamiento del puntero.
                                
                                x_puntero = 16 + sprite_get_width(sp_cartel_colgante_chico) + 20;
                                y_puntero = 8 + ( sprite_get_height(sp_cartel_colgante_chico) / 2 );
                                
                            } 
                            else{ // a la opción que tiene justo arriba (considerando si está sobre el bloque sector 7 especial)
                            
                                if( menu_opcion != 6 ){ menu_opcion -= 2; }
                                else{ menu_opcion -= 1; }
                                
                                // Posicionamiento del puntero 
                                x_puntero += 32 + sprite_get_width( sp_bloque_sector );
                                y_puntero = y_puntero - sprite_get_height(sp_bloque_sector) - 112;
                                
                            }
                    
                        }
                        
                        // Se verifica sobre que opción esta y a cual bloque de sector mostrár su planilla
                        with( ob_bloque_sector ){
                            
                            if( other.menu_opcion == numero ){ mostrar_planilla = true; }
                            else{ mostrar_planilla = false; }
                            
                        }
                    
                        // Se inhabilita el movimiento en el menú temporalmente
                        menu_move = false;
                        alarm[1] = 10;
                        
                        //[Sonido]
                        audio_play_sound( sn_menu_move, 10, false );
                        
                        break;
                        
                    }
                    
                    //----&gt; Al moverse hacia abajo
                    if( tecla_menu_abajo == true ){
                    
                        //Se verifica si está en la primera fila de sectores, segunda fila o la opción volver (igual a 0).
                        if( menu_opcion == 0 ){ // primera opcion de la primera fila
                        
                            menu_opcion = 1;
                            
                            // Se consigue la posición del bloque de sector 4
                            with( ob_bloque_sector ){
                            
                                if( numero = 1 ){
                                    
                                    // Posicionamiento del puntero.
                                    other.x_puntero = x + sprite_get_width(sp_bloque_sector) + 20; 
                                    other.y_puntero = y + ( sprite_get_height(sp_bloque_sector) / 2 );
                                    
                                }
                                
                            } 
                            
                        } 
                        else{
                        
                            if( menu_opcion == 6 ){ // Se va a la opción de volver atrás
                            
                                menu_opcion = 0; 
                                    
                                // Posicionamiento del puntero.
                                x_puntero = 16 + sprite_get_width(sp_cartel_colgante_chico) + 20;
                                y_puntero = 8 + ( sprite_get_height(sp_cartel_colgante_chico) / 2 );
                            
                            }
                            else{
                            
                                if( ( menu_opcion + 2) &gt;= cantidad_opciones ){ // Al bloque sector 6 (especial)
                                
                                    menu_opcion = 6; 
                                    
                                    // Se consigue la posición del bloque de sector 6 (especial)
                                    with( ob_bloque_sector ){
                                    
                                        if( numero == 6 ){
                                            
                                            // Posicionamiento del puntero.
                                            other.x_puntero = x + sprite_get_width(sp_bloque_sector) + 20; 
                                            other.y_puntero = y + ( sprite_get_height(sp_bloque_sector) / 2 );
                                            
                                        }
                                        
                                    }
                                    
                                } 
                                else{ // a la opción que está  debajo.
                                
                                    if( menu_opcion &lt; 3 ){
                                        
                                        // Posicionamiento del puntero horizontalmente
                                        if( menu_opcion == 1 ){ x_puntero += 32 + sprite_get_width( sp_bloque_sector ); }
                                        else{ x_puntero -= 32 + sprite_get_width( sp_bloque_sector ); }
                                        
                                        menu_opcion = 4; 
                                        
                                    
                                    }
                                    else{
                                    
                                        menu_opcion = 5; 
                                        
                                        // Posicionamiento del puntero horizontalmente
                                        x_puntero -= ( 32 + sprite_get_width( sp_bloque_sector ) );
                                    
                                    }
                                    
                                    // Posicionamiento del puntero verticalmente
                                    y_puntero = y_puntero  + sprite_get_height(sp_bloque_sector) + 112;
                                    
                                } 
                                
                            
                            }
                        
                            
                        
                        }
                        
                        // Se verifica sobre que opción esta y a cual bloque de sector mostrár su planilla
                        with( ob_bloque_sector ){
                            
                            if( other.menu_opcion == numero ){ mostrar_planilla = true; }
                            else{ mostrar_planilla = false; }
                            
                        }
                        
                        // Se inhabilita el movimiento en el menú temporalmente
                        menu_move = false;
                        alarm[1] = 10;
                    
                        //[Sonido]
                        audio_play_sound( sn_menu_move, 10, false );
                        
                        break;
                        
                    }
                    
                    
                    //----&gt; Al moverse hacia la izquierda
                    if( tecla_menu_izquierda == true ){
                    
                        // Se verificia que no este en la opcion de volver atrás o en la última opción
                        if( menu_opcion != 0 and menu_opcion != 6 ){
                        
                            /* Si se encuentra en la primera opción de la primera fila, se coloca en la última opción de la misma. Si se encuentra en la primera
                            opción de la segunda fila, se coloca en la última de lamisma. Si no es alguno de los casos anteriores, se mueve una posición a la 
                            izquierda.*/
                            if( menu_opcion == 1 or menu_opcion == 4 ){
                            
                                if( menu_opcion == 1 ){
                                
                                    menu_opcion += 2;
                                
                                    // Posicionamiento del puntero.
                                    x_puntero += 2 * ( 128 + sprite_get_width(sp_bloque_sector) );
                                   
                                }
                                else{
                                
                                    menu_opcion += 1;
                                
                                    // Posicionamiento del puntero.
                                    x_puntero +=  128 + sprite_get_width(sp_bloque_sector);
                                
                                }
                                    
                        
                            }
                            else{
                            
                                menu_opcion--;
                            
                                // Posicionamiento del puntero.
                                x_puntero -= 128 + sprite_get_width(sp_bloque_sector);
                            
                            }
                            
                            // Se verifica sobre que opción esta y a cual bloque de sector mostrár su planilla
                            with( ob_bloque_sector ){
                            
                                if( other.menu_opcion == numero ){ mostrar_planilla = true; }
                                else{ mostrar_planilla = false; }
                            
                            }
                            
                            // Se inhabilita el movimiento en el menú temporalmente
                            menu_move = false;
                            alarm[1] = 10;
                            
                            //[Sonido]
                            audio_play_sound( sn_menu_move, 10, false );
                            
                            
                        }
                        
                        break;
                    
                    }
                    
                    //----&gt; Al moverse hacia la derecha
                    if( tecla_menu_derecha == true ){
                    
                        // Se verificia que no este en la opcion de volver atrás ni en la última opción
                        if( menu_opcion != 0 and menu_opcion != 6 ){
                        
                            /* Si se encuentra en la primera opción de la primera fila, se coloca en la última opción de la misma. Si se encuentra en la primera
                            opción de la segunda fila, se coloca en la última de lamisma. Si no es alguno de los casos anteriores, se mueve una posición a la 
                            izquierda.*/
                            if( menu_opcion == 3 or menu_opcion == 5 ){
                            
                                if( menu_opcion == 3 ){
                                
                                    menu_opcion -= 2;
                                
                                    // Posicionamiento del puntero.
                                    x_puntero -= 2 * ( 128 + sprite_get_width(sp_bloque_sector) );
                                    
                                }
                                else{
                                
                                    menu_opcion -= 1;
                                
                                    // Posicionamiento del puntero.
                                    x_puntero -= 128 + sprite_get_width(sp_bloque_sector);
                                
                                }
                            
                        
                            }
                            else{
                            
                                menu_opcion++;
                            
                                // Posicionamiento del puntero.
                                x_puntero += 128 + sprite_get_width(sp_bloque_sector);
                            
                            }
                            
                            // Se verifica sobre que opción esta y a cual bloque de sector mostrár su planilla
                            with( ob_bloque_sector ){
                            
                                if( other.menu_opcion == numero ){ mostrar_planilla = true; }
                                else{ mostrar_planilla = false; }
                            
                            }
                        
                            // Se inhabilita el movimiento en el menú temporalmente
                            menu_move = false;
                            alarm[1] = 10;
                            
                            //[Sonido]
                            audio_play_sound( sn_menu_move, 10, false );
                            
                        }
                        
                        break;
                    
                    }
                    
                    // Al seleccionar una opción
                    if( tecla_menu_seleccionar == true ){
                            
                        // Se desactiva temporalmente el uso del menu y se llama a la alarma que manejo el uso de opciones
                        menu_move = false;
                        alarm[1] = 10;
                        event_perform(ev_alarm, 2);
                        
                        break;
                            
                    }
                    
                    // Al cancelar una opción (se vuelve a la opción "Volver" si no está ya en él)
                    if( tecla_menu_volver == true and menu_opcion != 0 ){
                    
                        // Se desactiva temporalmente el uso del menu y se llama a la alarma que manejo la cancelación de opciones.
                        menu_move = false;
                        alarm[1] = 10;
                        event_perform(ev_alarm, 3);
                        
                        break;
                    
                    }
            
                }
                
                break;
            
            }
            
            
            /*-----------------------------------
            
                &lt;&lt;&lt; Selección de Niveles &gt;&gt;&gt;
            
            -----------------------------------*/
            case room_seleccion_niveles:{
            
                // Se guardan las entradas de teclado para aceptar o cancelar una acciones
                tecla_menu_seleccionar = keyboard_check_released(tecla_jump);
                tecla_menu_volver = keyboard_check_released(tecla_salir);
            
                // Se controla que las ambas teclas anteriores no puedan actuar a la vez
                if( tecla_menu_volver == true and tecla_menu_seleccionar == true ){
                
                    tecla_menu_volver = false;
                    tecla_menu_seleccionar = false;    
                
                }
            
                // Se verifica si el jugador puede moverse en el menú.
                if( menu_move == true ){
                
                    // Se guardan las entradas de teclado para movimiento necesarias
                    tecla_menu_arriba = keyboard_check(tecla_up);
                    tecla_menu_abajo = keyboard_check(tecla_down);
                    tecla_menu_izquierda = keyboard_check(tecla_left);
                    tecla_menu_derecha = keyboard_check(tecla_right);                    
                        
                    // En el caso que valgan verdadero ambas teclas (arriba y abajo), se toman como falsas luego.
                    if( tecla_menu_arriba == true and tecla_menu_abajo == true ){
                        
                        tecla_menu_arriba = false;
                        tecla_menu_abajo = false;
                        
                    }
                    
                    // En el caso que valgan verdadero ambas teclas (izquierda y derecha), se toman como falsas luego.
                    if( tecla_menu_izquierda and tecla_menu_derecha ){
                    
                        tecla_menu_izquierda = false;
                        tecla_menu_derecha = false;
                    
                    }
                    
                    //----&gt; Al moverse hacia arriba
                    if( tecla_menu_arriba == true ){ 
                    
                        // Se verifica en que línea se encuentra
                        if( menu_opcion == 0 ){ // Se va al nivel especial
                        
                            /* Si esta en el primer sector, no existe la opción "anterior sector" (en ese caso va al de "siguiente sector), caso contrario
                            se va a dicho cartel.*/
                            if( sector_actual != 1 ){
                            
                                menu_opcion = 12;
                                
                                // Posicionamiento del puntero.
                                x_puntero = 16 + sprite_get_width(sp_anterior_siguiente_sector) + 20;
                                y_puntero = room_alto - ( sprite_get_height(sp_anterior_siguiente_sector) / 2 );
                                
                            }
                            else{
                            
                                //Si el siguiente sector esta desbloqueado se mueve a la opción correspondiente, caso contario se va al ultimo nivel
                                if( opcion_siguiente_sector == true ){
                                
                                    menu_opcion = 13;
                                    
                                    // Posicionamiento del puntero.
                                    x_puntero = 568 - 20;
                                    y_puntero = room_alto - ( sprite_get_height(sp_anterior_siguiente_sector) / 2 ); 
                                
                                }
                                else{
                                
                                    menu_opcion = 11;
                                
                                    // Se consigeue la posición del bloque de nivel 11(especial)
                                    with( ob_bloque_nivel ){
                                            
                                        if( numero == 11 ){
                                        
                                            // Posicionamiento del puntero.
                                            other.x_puntero = x + sprite_get_width(sp_bloque_nivel) + 20;
                                            other.y_puntero = y + ( sprite_get_height(sp_bloque_nivel) / 2 );
                                        
                                        }
                                        
                                    }
                                    
                                }
                                
                            }
                            
                        }
                        else{
                        
